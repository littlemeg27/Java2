<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ravenmargret.java2project4a" >

    <permission android:name="com.company.android.provider.ACCESS_DATA"
        android:protectionLevel="dangerous" />
    <uses-permission android:name="com.company.android.provider.ACCESS_DATA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PersonListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FormActivity"
            android:label="@string/title_activity_form" >
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail" >
        </activity>

        <provider android:name=".CRUDProvider"
            android:authorities="com.example.ravenmargret.java2project4a.CRUDProvider"
            android:enabled="true"
            android:exported="true"
            android:permission="com.company.android.provider.ACCESS_DATA" >
        </provider>
    </application>

</manifest>
